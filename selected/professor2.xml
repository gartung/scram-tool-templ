<tool name="professor2" version="${PROFESSOR2_VER}">
<client>
<environment name="PROFESSOR2_BASE" default="${PROFESSOR2_PREFIX}"/>
</client>
<use name="py2-numpy"/>
<use name="py2-sympy"/>
<use name="py2-matplotlib"/>
<use name="root"/>
<use name="yoda"/>
<use name="eigen"/>
<runtime name="PATH" value="$$PROFESSOR2_BASE/bin" type="path"/>
<runtime name="PYTHONPATH" value="$$PROFESSOR2_BASE/lib/python2.7/site-packages" type="path"/>
</tool>
