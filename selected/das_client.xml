<tool name="das_client" version="${DAS_CLIENT_VER}">
  <client>
    <environment name="DAS_CLIENT_BASE" default="${DAS_CLIENT_PREFIX}"/>
  </client>
  <runtime name="PATH"       value="$$DAS_CLIENT_BASE/bin" type="path"/>
  <runtime name="PYTHONPATH" value="$$DAS_CLIENT_BASE/bin" type="path"/>
  <use name="python"/>
</tool>
