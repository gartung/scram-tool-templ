<tool name="cfe_bindings" version="$CFE_VER">
  <client>
    <environment name="CFE_BASE" value="$CFE_PREFIX"/>
    <environment name="INCLUDE" value="$$CFE_BASE/include"/>
    <environment name="LIBDIR" value="$$CFE_BASE/lib"/>
    <environment name="PYTHONPATH" value="$$CFE_BASE/lib/python2.7/site-packages/" type="path"/>
  </client>
  <runtime name="DYLD_FALLBACK_LIBRARY_PATH" value="$$LIBDIR" type="path"/>
  <runtime name="PYTHONPATH" value="$$CFE_BASE/lib/python2.7/site-packages/" type="path"/>
  <use name="python"/>
  <flags SKIP_TOOL_SYMLINKS="1"/>
</tool>
